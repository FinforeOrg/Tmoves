<h1>Listing tweet_users</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Followers count</th>
    <th>Statuses count</th>
    <th>Url</th>
    <th>Location</th>
    <th>Friends count</th>
    <th>Favourites count</th>
    <th>Time zone</th>
    <th>Is protected</th>
    <th>Profile image url</th>
    <th>Screen name</th>
    <th>Lang</th>
    <th>Name</th>
    <th>Notifications</th>
    <th>Twitter</th>
    <th>Joined at</th>
    <th>Listed count</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tweet_users.each do |tweet_user| %>
  <tr>
    <td><%= tweet_user.description %></td>
    <td><%= tweet_user.followers_count %></td>
    <td><%= tweet_user.statuses_count %></td>
    <td><%= tweet_user.url %></td>
    <td><%= tweet_user.location %></td>
    <td><%= tweet_user.friends_count %></td>
    <td><%= tweet_user.favourites_count %></td>
    <td><%= tweet_user.time_zone %></td>
    <td><%= tweet_user.is_protected %></td>
    <td><%= tweet_user.profile_image_url %></td>
    <td><%= tweet_user.screen_name %></td>
    <td><%= tweet_user.lang %></td>
    <td><%= tweet_user.name %></td>
    <td><%= tweet_user.notifications %></td>
    <td><%= tweet_user.twitter_id %></td>
    <td><%= tweet_user.joined_at %></td>
    <td><%= tweet_user.listed_count %></td>
    <td><%= link_to 'Show', tweet_user %></td>
    <td><%= link_to 'Edit', edit_tweet_user_path(tweet_user) %></td>
    <td><%= link_to 'Destroy', tweet_user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Tweet user', new_tweet_user_path %>
