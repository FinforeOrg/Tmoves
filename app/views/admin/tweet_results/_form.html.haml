= form_for(@tweet_result) do |f|
  - if @tweet_result.errors.any?
    #error_explanation
      %h2
        = pluralize(@tweet_result.errors.count, "error")
        prohibited this tweet_result from being saved:
      %ul
        - @tweet_result.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label :tweet_id
    %br/
    = f.text_field :tweet_id
  .field
    = f.label :retweet_count
    %br/
    = f.text_field :retweet_count
  .field
    = f.label :coordinates
    %br/
    = f.text_area :coordinates
  .field
    = f.label :geo
    %br/
    = f.text_area :geo
  .field
    = f.label :place
    %br/
    = f.text_area :place
  .field
    = f.label :source
    %br/
    = f.text_area :source
  .field
    = f.label :created_at
    %br/
    = f.datetime_select :created_at
  .field
    = f.label :tweet_text
    %br/
    = f.text_area :tweet_text
  .field
    = f.label :keywords
    %br/
    = f.text_area :keywords
  .field
    = f.label :lang
    %br/
    = f.text_field :lang
  .actions
    = f.submit