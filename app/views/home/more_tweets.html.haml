- if @results.length > 0
  = render :partial => "shared/tweet", :collection=>@results, :locals => { :is_hidden => @is_hidden }
  %tr{:class=>"more_tweets_info#{@div_tag}", :style=>"display:none;"}
  :javascript
    $(document).ready(function(){
          $("a.twtName").fancybox({
                'titleShow'     : false,
                'transitionIn'  : 'elastic',
                'transitionOut' : 'elastic',
                'easingIn'      : 'easeOutBack',
                'easingOut'     : 'easeInBack'
          });
    });