<% content_for :title, "Subscribe Report" %>

<div id="login-panel">
  <span id="title">Subscribe Tmoves Report</span><br />
  <span id="login-subtitle">Please enter your name and email:</span>
  <div id="login-form">
    <%= form_for("admin", :url => new_member_path) do |f| %>
      <div id="login-username" class="right">Full Name</div>
      <%= f.text_field :name %>
      <span id="admin_email-error"><img class="error" src="images/button-error.png" />Error username</span>
      <div class="login-delimiter">&nbsp;</div>
      <div id="login-password" class="right">Email</div>
      <%= f.email_field :email %>
      <span id="admin_password-error"><img class="error" src="images/button-error.png" />Error password</span>
      <%= f.submit 'Subscribe', {:id => 'login-signin', :class => 'button'} %>
    <% end %>
    <div class="login-delimiter">&nbsp;</div>
  </div>
</div>

